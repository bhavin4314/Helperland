                            <li class="nav-item "><a class="nav-link nav-link-2" href="#">Contact</a></li>
                            <div class="line1"></div>
                            <img src="~/images/icon-notification.png" class="nav-image1" alt="" srcset="">
                            <div class="notification-number">2</div>
                            <div class="line2"></div>
                            
                            <li class="nav-item dropdown-list">
                              <a class="nav-link " id="navbar-dropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <img src="~/images/user.png" class="nav-image2" alt="" srcset="">
                                <img src="~/images/forma-1-copy@2x.png" class="nav-image3" alt="" srcset="">
                              </a>
                              <ul class="dropdown-menu dropdown-menu-right">
                                <li class="list-item1"><a href="" disabled>Welcome, <br><span class="font-weight-bold">Sandip Patel</span></a></li>
