                                <li class="dropdown-item" type="button">My Settings</li>
                                <li class="dropdown-item" type="button">Logout</li>
                              </ul>
                            </li>
                            
                            <span class="collapse-menu-item">
                              <hr>
                              <div class="d-flex justify-content-center mobile-icon">
                                <a href=""><img src="~/images/instagram-mobile-menu.png" class="mx-3" alt="" srcset=""></a>
                                <a href=""><img src="~/images/facebook-mobile-menu.png" class="mx-3" alt="" srcset=""></a>
                                
                              </div>
                              
                            </span>
                    </ul>
                </div>
